{
  "name": "svelte-mock",
  "version": "0.0.0-alpha.0",
  "description": "A package for mocking svelte components with jest.",
  "main": "lib/index.js",
  "files": [
    "lib",
    "extend.js",
    "transform.js"
  ],
  "directories": {
    "lib": "lib",
    "test": "test"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/kobejean/svelte-mock.git"
  },
  "keywords": [
    "Svelte",
    "Jest",
    "Mock"
  ],
  "author": "Jean A. Flaherty",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/kobejean/svelte-mock/issues"
  },
  "homepage": "https://github.com/kobejean/svelte-mock#readme",
  "jest": {
    "transform": {
      "\\.js$": "babel-jest",
      "\\.html$": "<rootDir>/transform.js",
      "\\.svelte$": "<rootDir>/transform.js"
    },
    "setupFilesAfterEnv": [
      "<rootDir>/extend.js"
    ],
    "moduleFileExtensions": [
      "js",
      "json",
      "html"
    ],
    "testRegex": "/test/.+\\.spec\\.js$"
  },
  "scripts": {
    "format": "prettier --write \"{lib,test}/**/*.js\"",
    "test": "jest",
    "test:watch": "jest --watch",
    "prepublishOnly": "npm ci && npm run test"
  },
  "devDependencies": {
    "@babel/core": "^7.5.5",
    "@babel/plugin-proposal-object-rest-spread": "^7.5.5",
    "@babel/plugin-transform-runtime": "^7.5.5",
    "@babel/preset-env": "^7.5.5",
    "jest": "^25.0.0",
    "lodash": "^4.17.15",
    "node-version-compare": "^1.0.2",
    "prettier": "1.13.3",
    "svelte": "^2.0.0"
  },
  "peerDependencies": {
    "svelte": "*"
  },
  "dependencies": {
    "@babel/polyfill": "^7.4.4",
    "@babel/runtime": "^7.5.5"
  }
}
