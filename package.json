{
  "name": "svelte-mock",
  "version": "0.0.0-alpha.9",
  "description": "A package for mocking svelte components with jest.",
  "main": "lib/index.js",
  "files": [
    "lib",
    "extend.js",
    "transform.js",
    "transform-debug.js"
  ],
  "directories": {
    "lib": "lib",
    "test": "test"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/kobejean/svelte-mock.git"
  },
  "keywords": [
    "Svelte",
    "Jest",
    "Mock"
  ],
  "author": "Jean A. Flaherty",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/kobejean/svelte-mock/issues"
  },
  "homepage": "https://github.com/kobejean/svelte-mock#readme",
  "scripts": {
    "build": "node_modules/@babel/cli/bin/babel.js src --out-dir lib",
    "format": "prettier --write \"{lib,test}/**/*.js\"",
    "test": "jest",
    "test:watch": "jest --watch",
    "prepublishOnly": "npm ci && npm run build && npm run test"
  },
  "devDependencies": {
    "@babel/cli": "^7.5.5",
    "@babel/core": "^7.5.5",
    "@babel/plugin-transform-runtime": "^7.5.5",
    "@babel/preset-env": "^7.5.5",
    "jest": "^25.0.0",
    "prettier": "1.13.3",
    "svelte": "^2.0.0"
  },
  "peerDependencies": {
    "svelte": "*",
    "jest": "*"
  },
  "dependencies": {
    "@babel/polyfill": "^7.4.4",
    "@babel/runtime": "^7.5.5",
    "babel-plugin-module-resolver": "^3.2.0",
    "compare-versions": "^3.5.1",
    "lodash": "^4.17.15",
    "regexp-sourcemaps": "^1.0.1"
  }
}
